<p id="notice"><%= notice %></p>

<h1>Listing Multi Spec Tags</h1>

<table>
  <thead>
    <tr>
      <th>Multi spec sample</th>
      <th>Tag</th>
      <th>Tagvalue</th>
      <th>Upload status</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @multi_spec_tags.each do |multi_spec_tag| %>
      <tr>
        <td><%= multi_spec_tag.multi_spec_sample_id %></td>
        <td><%= multi_spec_tag.Tag %></td>
        <td><%= multi_spec_tag.TagValue %></td>
        <td><%= multi_spec_tag.upload_status %></td>
        <td><%= link_to 'Show', multi_spec_tag %></td>
        <td><%= link_to 'Edit', edit_multi_spec_tag_path(multi_spec_tag) %></td>
        <td><%= link_to 'Destroy', multi_spec_tag, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Multi spec tag', new_multi_spec_tag_path %>
